<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

  <!-- versionCode is optional and Android only -->

  <name>DnnConsulting App Test</name>

  <description>
    An example of a Native app.
  </description>

  <author href="https://www.dnnconsulting.nlm" email="modules@dnnconsulting.nl">
    Geoff Barlow
  </author>

  <icon src="images/res/icon.jpg" />
  <icon src="images/res/icon.jpg" gap:platform="android"    gap:density="ldpi" />
  <icon src="images/res/icon.jpg" gap:platform="android"    gap:density="mdpi" />
  <icon src="images/res/icon.jpg" gap:platform="android"    gap:density="hdpi" />
  <icon src="images/res/icon.jpg" gap:platform="android"    gap:density="xhdpi" />
  <icon src="images/res/icon.jpg" gap:platform="blackberry" />
  <icon src="images/res/icon.jpg" gap:platform="blackberry" gap:state="hover"/>
  <icon src="images/res/icon.jpg" gap:platform="ios"        width="57" height="57" />
  <icon src="images/res/icon.jpg" gap:platform="ios"        width="72" height="72" />
  <icon src="images/res/icon.jpg" gap:platform="ios"        width="114" height="114" />
  <icon src="images/res/icon.jpg" gap:platform="ios"        width="144" height="144" />
  <icon src="images/res/icon.jpg" gap:platform="webos" />
  <icon src="images/res/icon.jpg" gap:platform="winphone" />
  <icon src="images/res/icon.jpg" gap:platform="winphone"   gap:role="background" />

  <platform name="ios" />
  <platform name="android" />
  <platform name="winphone" />
  
  <!--<access origin="http://demo.cmsx1.nl" subdomains="true"/>-->
  <access origin="http://demo.cmsx1.nl" subdomains="true" browserOnly="false" /> 
</widget>